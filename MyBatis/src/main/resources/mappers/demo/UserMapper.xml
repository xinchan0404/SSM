<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.xinchan.mybatis.mapper.demo.UserMapper">
	<!--int insertUser();-->
	<insert id="insertUser">
		insert into t_user values(null, "xinchan", "123456", 25, "male", "zhengxin0404@foxmail.com");
	</insert>

	<!--int updateProduct();-->
	<update id="updateUser">
		update t_user set username = "zhengxin" where id = 3;
	</update>

	<!--void deleteUser();-->
	<delete id="deleteUser">
		delete from t_user where id = 2;
	</delete>

	<!--User selectUser();-->
	<select id="selectUser" resultType="pers.xinchan.mybatis.pojo.User">
		select * from t_user where id = 3;
	</select>

	<!--List<User> selectAllUser();-->
	<select id="selectAllUser" resultType="pers.xinchan.mybatis.pojo.User">
		select * from t_user;
	</select>
</mapper>